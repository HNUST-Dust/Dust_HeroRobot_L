{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DAP-link",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/build/Dust_HeroRobot_L.elf",
            "request": "launch",
            "type": "cortex-debug",
            "device": "STM32F407IGHX",
            "servertype": "openocd",
            "configFiles": [
                "${workspaceRoot}/Core/config/openocd_dap.cfg",
            ],
            "runToEntryPoint": "main",
        },
        {
            "name": "DAP-Attach",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/build/Dust_HeroRobot_L.elf",
            "request": "attach",
            "type": "cortex-debug",
            "device": "STM32F407IGHX",
            "servertype": "openocd",
            "configFiles": [
                "${workspaceRoot}/Core/config/openocd_dap.cfg",
            ],
        },
    ]
}